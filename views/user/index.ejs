<div class="box">
  <div class="box-header with-border">
    <h3 class="box-title">Lista de Usuarios</h3>
  </div>
  <div class="box-body">
    <table class="table table-bordered">
      <tr>
        <th></th>
        <th>email</th>
        <th>nombres</th>
        <th>apellidos</th>
        <th>password</th>
        <th>seccion</th>
        <th>rut</th>
        <th>estado</th>
      </tr>
      <% users.forEach( function( model ){ %>
        <tr>
          <td>
            (<a href="/usuarios/delete/<%= model.id %>">Eliminar</a>
            |
            <a href="/usuarios/update/<%= model.id %>">Update</a>
            |
            <a href="/usuarios/view/<%=model.id %>">view</a>)
          </td>
          <td><%= model.email %></td>
          <td><%= model.nombres %></td>
          <td><%= model.apellidos %></td>
          <td><%= model.password %></td>
          <td><%= model.seccion %></td>
          <td><%= model.rut %></td>
          <td><%= model.estado %></td>
        </tr>
      <% }); %>
    </table>
  </div>
</div>
