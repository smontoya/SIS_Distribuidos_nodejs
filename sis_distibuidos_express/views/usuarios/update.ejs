

<div class="box box-primary">
  <div class="box-header with-border">
    <h3 class="box-title">Editar Usuario</h3>
  </div>
  <!-- /.box-header -->
  <!-- form start -->
  <form role="form" method="POST">
    <div class="box-body">
      <div class="form-group">
        <label for="exampleInputEmail1">e-mail</label>
        <input type="text" class="form-control" name="User[email]" value="<%=model.email%>" placeholder="Ingrese su email">
      </div>
      <div class="form-group">
        <label for="exampleInputEmail1">Nombre(s)</label>
        <input type="text" class="form-control" name="User[nombres]" value="<%=model.nombres%>" placeholder="Ingrese su(s) Nombre(s)">
      </div>
      <div class="form-group">
        <label for="exampleInputEmail1">Apellido(s)</label>
        <input type="text" class="form-control" name="User[apellidos]" value="<%=model.apellidos%>" placeholder="Ingrese su(s) apellido(s)">
      </div>
      <div class="form-group">
        <label for="exampleInputEmail1">Sección</label>
        <input type="text" class="form-control" name="User[seccion]" value="<%=model.seccion%>" placeholder="Ingrese su sección">
      </div>
      <div class="form-group">
        <label for="exampleInputEmail1">RUN</label>
        <input type="text" class="form-control" name="User[rut]" value="<%=model.rut%>" placeholder="Ingrese su RUN">
      </div>
      <div class="checkbox">
        <label>
          <input id="" type="checkbox" name="User[estado]" <%if (model.estado){%>checked<% } %>> Habilitado
        </label>
      </div>

    </div>

    <div class="box-footer">
      <button type="submit" class="btn btn-primary">Actualizar</button>
    </div>
  </form>
</div>

<a href="/usuarios" title="Listar Usuarios" class="btn btn-primary">
  Listar Usuarios
</a>