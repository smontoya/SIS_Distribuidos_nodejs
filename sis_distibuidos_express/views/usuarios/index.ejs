
<div class="box">
  <div class="box-header with-border">
    <h3 class="box-title">Lista de Usuarios</h3>
  </div>
  <div class="box-body">
    <table class="table table-bordered">
      <tr>
        <th></th>
        <th>email</th>
        <th>nombres</th>
        <th>apellidos</th>
        <th>password</th>
        <th>seccion</th>
        <th>rut</th>
        <th>Habilitado</th>
      </tr>
      <% users.forEach( function( model ){ %>
        <tr>
          <td>
            <a href="/usuarios/delete/<%= model.id %>" title="Eliminar Usuario" class="delete_user"><i class="fa fa-trash"></i></a>
            <a href="/usuarios/update/<%= model.id %>" title="Editar Usuario"><i class="fa fa-pencil"></i></a>
            <a href="/usuarios/view/<%=model.id %>" title="Ver usuario"><i class="fa fa-eye"></i></a>
          </td>
          <td><%= model.email %></td>
          <td><%= model.nombres %></td>
          <td><%= model.apellidos %></td>
          <td><%= model.password %></td>
          <td><%= model.seccion %></td>
          <td><%= model.rut %></td>
          <td><%= model.estado %></td>
        </tr>
      <% }); %>
    </table>
  </div>
</div>



<a href="/usuarios/create" title="Crear Usuario" class="btn btn-primary">Crear Usuario</a>
<a href="/usuarios" title="Listar Usuario" class="btn btn-primary">Listar Usuario</a>